{
  "name": "back-server",
  "version": "1.0.0",
  "description": "Backend: NodeJS, Express, Sequelize, MySQL, JWT, Bcrypt, Nodemon, Morgan, Cors, Express-Validator, Axios, Sequelize-Auto y Sequelize-CLI",
  "type": "module",
  "author": "Daniel Covarrubias <dacovasan00@gmail.com>",
  "license": "MIT",
  "main": "src/app.js",
  "scripts": {
    "start": "NODE_ENV=PROD node src/app.js",
    "dev": "NODE_ENV=DEV node --watch src/app.js",
    "test": "NODE_ENV=TEST NODE_OPTIONS=--experimental-vm-modules jest --detectOpenHandles",
    "test:watch": "npm run test -- --watch",
    "create-models": "node src/config/config-sequelize-auto.js",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo:all",
    "seed:all": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all"
  },
  "dependencies": {
    "axios": "1.5.0",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-validator": "7.0.1",
    "jsonwebtoken": "9.0.2",
    "mongoose": "7.5.0",
    "sequelize": "6.33.0",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "eslint": "8.49.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-n": "16.0.2",
    "eslint-plugin-promise": "6.1.1",
    "jest": "^29.6.4",
    "jest-extended": "4.0.1",
    "morgan": "1.10.0",
    "nodemon": "3.0.1",
    "supertest": "6.3.3"
  }
}